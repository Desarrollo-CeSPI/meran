</script>
<fieldset>
<legend class="titulo_legend_resaltado">Listado de Proveedores</legend>
<div class="buttonContainerHorizontal">
<ul>
    <li class="horizontal">
            <input id="nombre_proveedor" type=text name="nombre_proveedor" onFocus="checkFilter('focus')" 
                onBlur="checkFilter('blur')">
            [% PERL %]
                                print C4::AR::Filtros::to_Button(   text    => "[% 'Buscar' | i18n %]",
                                                                    boton   => "boton_buscar",
                                                                    onClick => "consultar();",
                                                                    title   => "[% 'Buscar' | i18n %]",
                                        ) ;
            [% END %]
            [% PERL %]
                                print C4::AR::Filtros::to_Button(   text    => "[% 'Limpiar' | i18n %]",
                                                                    boton   => "boton_buscar",
                                                                    onClick => "borrar();",
                                                                    title   => "[% 'Limpiar' | i18n %]",
                                        ) ;
            [% END %]
    </li>
</ul>
</div>
<ul>
    [% PERL %]
         print C4::AR::Filtros::setHelp(text => "[% 'Tipee el nombre o la razon social del proveedor que desea buscar.' | i18n %]",
